describe(
		'Case',
		function() {
			it(
					'test',
					function() {
						var url = 'https://www.easypolicy.com/';
						browser.manage().window().maximize();
						browser.get(url);
						expect(browser.getTitle())
								.toEqual(
										'Easypolicy - #1 from Insurance Comparison to Claim Support');

					})

			it(
					'click on term',
					function() {
						var url = 'https://www.easypolicy.com/';
						browser.get(url);

						browser.manage().window().maximize();
						browser.sleep(5000);

						element(
								by
										.xpath("//div[@class='producttext'][contains(text(),'Term')]"))
								.click()

						element(
								by
										.xpath('/html[1]/body[1]/section[8]/div[1]/div[1]/div[1]/div[1]/div[2]/div[1]/div[1]/md-input-container[1]/a[2]'))
								.click();
						element(by.xpath("//a[contains(text(),'Yes')]"))
								.click();
						element(by.id('input-64')).click();
						element(by.xpath("//input[@id='input-64']")).sendKeys("10");
						browser.sleep(5000);
						$$('.md-autocomplete-suggestions li').then(function(items) {
							expect(items.length).toBe(1);
							expect(items[0].getText()).toBe('10 Lacs');
							items[0].click();
							browser.sleep(5000);
						})

					});

		})